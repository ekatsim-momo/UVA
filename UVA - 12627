#include<bits/stdc++.h>
using namespace std;
long long a[33],num[50];
int main()
{
    long long n,l,r,k,count=0;
    memset(a,0,sizeof(a));
    a[0]=1;
    num[0]=1;
    for(long long i=1; i<=30; i++)
    {
        a[i]=a[i-1]*3;
        num[i]=num[i-1]*2;
    }
    scanf("%lld",&n);
    while(n--)
    {
        scanf("%lld%lld%lld",&k,&l,&r);
        long long sum=a[k],ce=0,nu=1;
        if(k!=0)
        {
            nu=2;
            sum-=a[k-1]*2;
            if(l<=num[k]/2+1)
            {
                l=num[k]/2+1-l;
                while((1<<ce)<l)
                    ce++;
                while(ce>=0)
                {
                    if((1<<ce)&l)
                    {
                        sum+=a[ce]*nu;
                        nu*=2;
                    }
                    ce--;
                }
            }
            else
            {
                sum-=a[k-1];
                nu=1;
                l-=num[k]/2;
                 l=num[k]/2+1-l;
                while((1<<ce)<l)
                    ce++;
                while(ce>=0)
                {
                    if((1<<ce)&l)
                    {
                        sum+=a[ce]*nu;
                        nu*=2;
                    }
                    ce--;
                }
            }
            ce=0;
            nu=1;
            if(r>=num[k]/2)
            {
                r-=num[k]/2;
                r=num[k]/2+1-r;
                while((1<<ce)<r-1)
                    ce++;
                while(ce>=0)
                {
                    if((1<<ce)&r-1)
                    {
                        sum-=a[ce]*nu;
                        nu*=2;
                    }
                    ce--;
                }
            }
            else
            {
                sum-=a[k-1];
                nu=2;
                r=num[k]/2+1-r;
                while((1<<ce)<r-1)
                    ce++;
                while(ce>=0)
                {
                    if((1<<ce)&r-1)
                    {
                        sum-=a[ce]*nu;
                        nu*=2;
                    }
                    ce--;
                }
            }
        }
        else
            sum=1;
        printf("Case %lld: %lld\n",++count,sum);
    }
    return 0;
}
